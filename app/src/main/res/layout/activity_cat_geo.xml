                <ImageView
                    android:layout_alignParentTop="true"
                    android:id="@+id/iv_angulos"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/mod1geoang1"
                    android:background="@android:color/black" />

                <TextView
                    android:layout_below="@+id/iv_angulos"
                    android:id="@+id/tv_angulos"
                    android:text="Geometria plana: ângulos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="16dp"
                    android:paddingTop="9dp"
                    android:textSize="24sp" />

                <TextView
                    android:layout_below="@+id/tv_angulos"
                    android:id="@+id/tv_des_angulos"
                    android:text="Aprenda mais sobre ângulos com o professor Davi Barros."
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="16dp"
                    android:paddingBottom="5dp"
                    android:textSize="14sp" />
            </RelativeLayout>
        </android.support.v7.widget.CardView>

